package com.hins.cloud.common.dto;

import java.io.Serializable;

/**
 * 专门用来存放用户session信息的对象
 *
 * @Author: dqk
 * @Date: 2019/8/13 15:20
 */
public class UserSession implements Serializable {
	private static final long serialVersionUID = -3431921876119257424L;
	private String id;
	private String name;
	private String mobile;
	private String imgPath;

	public String getId() {
		return id;
	}

	public UserSession setId(String id) {
		this.id = id;
		return this;
	}

	public String getName() {
		return name;
	}

	public UserSession setName(String name) {
		this.name = name;
		return this;
	}

	public String getMobile() {
		return mobile;
	}

	public UserSession setMobile(String mobile) {
		this.mobile = mobile;
		return this;
	}

	public String getImgPath() {
		return imgPath;
	}

	public UserSession setImgPath(String imgPath) {
		this.imgPath = imgPath;
		return this;
	}
}
